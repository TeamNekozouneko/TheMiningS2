function initDefine():
    clear {define::*}
    handleDefine()

function getDefine(defineName: string) :: object:
    return {define::%{_defineName}%}

function getDefineList(defineListName: string) :: objects:
    return {define::%{_defineListName}%::*}

function getIndexList(defineListName: string) :: objects:
    loop {define::%{_defineListName}%::*}:
        add loop-index parsed as integer to {_indexes::*}
    return {_indexes::*}


function getDistributionURL(path: string) :: string:
    return "https://raw.githubusercontent.com/TeamNekozouneko/TheMiningS2/refs/heads/master/distribution/%{_path}%"