on load:
    #テストケース記述欄
    testFail() if defineInitTestHandler() is false
    
    wait 3 second
    testSuccess()

#結果処理
function testFail():
    broadcast "[TheMiningS2] THEMININGS2_TEST_FAILED" #Actions用
    stop server
function testSuccess():
    broadcast "[TheMiningS2] All tests passed successfully."
    stop server